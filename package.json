{
  "name": "scany",
  "version": "1.3.0",
  "description": "Easily grab video data from user, channel, playlist, and video URLs!",
  "main": "index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "clean": "rimraf ./dist/** ./coverage",
    "build": "tsc",
    "watch": "concurrently npm:watch:*",
    "watch:build": "tsc --watch",
    "watch:test": "ava --watch",
    "test": "tsc && nyc --color -a ava -v",
    "debug": "tsc && node ./dist/debug",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "version": "git add -A",
    "postversion": "git push && git push --tags && npm publish"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/JimmyBoh/scany.git"
  },
  "author": "Jim Buck <jim@jimmyboh.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/JimmyBoh/scany/issues"
  },
  "homepage": "https://github.com/JimmyBoh/scany#readme",
  "devDependencies": {
    "@types/cheerio": "^0.22.9",
    "@types/debug": "0.0.31",
    "@types/node": "^10.12.7",
    "@types/puppeteer": "^1.10.0",
    "ava": "^0.25.0",
    "concurrently": "^4.0.1",
    "coveralls": "^3.0.2",
    "nyc": "^13.1.0",
    "typescript": "^3.1.6"
  },
  "dependencies": {
    "debug": "^4.1.0",
    "p-map": "^2.0.0",
    "pully-core": "^0.7.4",
    "url-pattern": "^1.0.3",
    "ytpl": "^0.1.14",
    "ytsr": "^0.1.8"
  },
  "ava": {
    "files": [
      "./dist/**/*.spec.js"
    ],
    "source": [
      "./dist/**/*.js"
    ]
  },
  "nyc": {
    "timeout": "2m",
    "include": [
      "dist/**/*.js"
    ],
    "exclude": [
      "dist/**/*.spec.js",
      "dist/debug.js"
    ]
  }
}
